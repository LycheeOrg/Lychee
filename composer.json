{
    "name": "lycheeorg/lychee-laravel",
    "description": "A great looking and easy-to-use photo-management-system you can run on your server, to manage and share photos.",
    "homepage": "https://lycheeorg.github.io/",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "ext-exif": "*",
        "ext-gd": "*",
        "ext-json": "*",
        "bepsvpt/secure-headers": "^5.3",
        "doctrine/dbal": "^2.8",
        "fideloper/proxy": "^4.0",
        "graham-campbell/markdown": "^10.3",
        "laravel/framework": "5.8.*",
        "laravel/tinker": "^1.0",
        "maennchen/zipstream-php": "^0.5.2",
        "php-ffmpeg/php-ffmpeg": "^0.13.0"
    },
    "require-dev": {
        "ext-imagick": "*",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "itsgoingd/clockwork": "^3.1",
        "laravel/homestead": "^8.2",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.0",
        "wnx/laravel-stats": "^1.9"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover",
            "@make_user_css"
        ],
        "make_user_css": [
            "touch public/dist/user.css"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
